{
  "manifest_version": 2,
  "name": "L1 Helper",
  "version": "1.0",
  "description": "Добавляет дату и текст в Summary при создании тикета",
  "permissions": [
    "contextMenus",
    "https://app.intercom.com/*"
    ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
